{
    "printWidth": 150,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "always",
    "semi": true,
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "quoteProps": "as-needed",
    "rangeStart": 0
}
